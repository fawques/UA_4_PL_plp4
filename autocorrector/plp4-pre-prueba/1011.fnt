class Crear{
 public double a[,];
 public int x,y;

 public int Reserva()
 {
   a=new double[2,5];
   x=2;
   y=5;
   return x*y;
 }
 public int Rellena(double semilla)
 {
   int total;
   total=0;
   for(int i=0 to x-1 step 1)
      for(int j=0 to y-1 step 1)
       {
         a[i,j]=semilla*(i+j);
         total=total+a[i,j];
       }
  return total;
 }
 public int Mostrar()
 {
   for(int i=0 to x-1 step 1)    
      for(int j=0 to y-1 step 1)    
      System.Console.WriteLine(a[i,j]);
 }
}

