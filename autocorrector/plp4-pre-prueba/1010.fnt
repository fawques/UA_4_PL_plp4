class A{
 public double x[,];
 private int f1,c1;

 public int reserva(double f,double c)
 {
   x=new double[2,3];
   f1=f;
   c1=c;
   return 1;
 }

 public int asigna(double inicia)
 {
  for(int i=0 to f1-1 step 1)
   for(int j=0 to c1-1 step 1)
    {
      x[i,j]=inicia*2.0;
      inicia=inicia+1;
    }
  return inicia;
 }
 public double calcula()
 {
  int res;
  res=0;
  for(int i=0 to f1-1 step 1)   
   for(int j=0 to c1-1 step 1)   
      res=res+x[i,j];
  return res;
 }
 
}

class B{
public A obj;
private bool vra;
public int reserva()
{
  obj=new A();
  return 1;
}
public int asigna(double f,double c,double inicia)
{
 int k;
 obj.reserva(f,c);

 k=obj.asigna(inicia);
 return k;
}
public double calcula()
{
   return obj.calcula();
}
}

class C{
public static void Main()
{
  B obj;
  int m;
  obj =new B();
